{
  "name": "bichard7-next-tests",
  "version": "1.0.0",
  "description": "Tests for Bichard",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --require steps --retry 5 --publish-quiet features",
    "test:local-next": "AWS_URL=\"http://localhost:4566\" WORKSPACE=local-next STACK_TYPE=next npm run test",
    "test:baseline": "STACK_TYPE=baseline npm run test",
    "test:browser": "HEADLESS=false npm run test",
    "test:browser-noteardown": "NO_TEARDOWN=true npm run test:browser",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-tests.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-tests/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-tests#readme",
  "dependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@cucumber/cucumber": "^7.2.1",
    "aws-sdk": "^2.904.0",
    "axios": "^0.21.1",
    "callsites": "^3.1.0",
    "date-fns": "^2.20.2",
    "expect": "^26.6.2",
    "ibm_db": "^2.7.3",
    "pg-promise": "^10.10.1",
    "puppeteer": "^8.0.0",
    "stompit": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier -w"
    ]
  }
}
