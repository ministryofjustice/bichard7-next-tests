{
  "name": "bichard7-next-tests",
  "version": "1.0.0",
  "description": "Tests for Bichard",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:features",
    "lint:js": "eslint --fix .",
    "lint:features": "gherkin-lint features",
    "test": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and not @AuditLog' features",
    "test:auditlogs": "echo $PNC_HOST && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@AuditLog and not @OnlyRunsOnPNC and not @Excluded' features",
    "test:preprodauditlogs": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and @AuditLog and not @Excluded' features",
    "test:preprod": "SKIP_PNC_VALIDATION=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and not @AuditLog and not @Excluded' features",
    "test:loadtest-ui": "PRINT_ENV=true RUN_PARALLEL=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --tags '@LoadTestUI' features",
    "test:loadtest-noui": "PRINT_ENV=true RUN_PARALLEL=true NO_UI=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --tags '@LoadTestNoUI' features",
    "test:chunk": "bash ./scripts/run_test_chunk.sh 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and not @AuditLog'",
    "test:chunk:conductor": "bash ./scripts/run_test_chunk.sh 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and not @AuditLog and not @ExcludedOnConductor'",
    "test:file": "node --tls-min-v1.0 ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet",
    "test:file-parallel": "./node_modules/.bin/cucumber-js --format @cucumber/pretty-formatter --require steps --no-strict --exit --publish-quiet --parallel",
    "test:must": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must and not @AuditLog' features",
    "test:must:auditlogs": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must and @AuditLog' features",
    "test:browser": "HEADLESS=false npm run test",
    "test:browser-noteardown": "NO_TEARDOWN=true npm run test:browser",
    "test:characterisation": "cd characterisation && jest --runInBand",
    "test:characterisation:bichard": "cd characterisation && USE_BICHARD=true  jest --runInBand",
    "test:nextUI": "nextUI=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@NextUI'",
    "test:nextUI:file": "nextUI=true node --tls-min-v1.0 ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --tags '@NextUI'",
    "prepare": "husky install",
    "update-deps": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-tests.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-tests/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-tests#readme",
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@cucumber/cucumber": "^9.0.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.56",
    "@types/jest": "^29.4.0",
    "aws-sdk": "^2.1333.0",
    "axios": "^1.3.4",
    "csv-parse": "^5.3.6",
    "date-fns": "^2.29.3",
    "expect": "^29.5.0",
    "fast-xml-parser": "^4.1.3",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "nunjucks": "^3.2.3",
    "pg-promise": "^11.4.1",
    "promise-poller": "^1.9.1",
    "puppeteer": "^19.7.4",
    "puppeteer-mass-screenshots": "^1.0.15",
    "stompit": "^1.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "gherkin-lint": "^4.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-check-updates": "^16.7.12",
    "prettier": "^2.8.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier -w"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ]
  }
}
