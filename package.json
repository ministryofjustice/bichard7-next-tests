{
  "name": "bichard7-next-tests",
  "version": "1.0.0",
  "description": "Tests for Bichard",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:features",
    "lint:js": "eslint --fix .",
    "lint:features": "gherkin-lint features",
    "test": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and not @AuditLog' features",
    "test:auditlogs": "./scripts/prepare-environment.sh && echo $PNC_HOST && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@AuditLog and not @OnlyRunsOnPNC and not @Excluded' features",
    "test:preprodauditlogs": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and @AuditLog and not @Excluded' features",
    "test:preprod": "./scripts/prepare-environment.sh && SKIP_PNC_VALIDATION=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and not @AuditLog and not @Excluded' features",
    "test:loadtest-ui": "./scripts/prepare-environment.sh && PRINT_ENV=true RUN_PARALLEL=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --tags '@LoadTestUI' features",
    "test:loadtest-noui": "./scripts/prepare-environment.sh && PRINT_ENV=true RUN_PARALLEL=true NO_UI=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --tags '@LoadTestNoUI' features",
    "test:chunk": "./scripts/prepare-environment.sh && bash ./scripts/run_test_chunk.sh",
    "test:file": "./scripts/prepare-environment.sh && node --tls-min-v1.0 ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet  --tags 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC'",
    "test:file-parallel": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --format @cucumber/pretty-formatter --require steps --no-strict --exit --publish-quiet --parallel",
    "test:local-next": "AWS_URL=\"http://localhost:4566\" WORKSPACE=local-next npm run test",
    "test:must": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must and not @AuditLog' features",
    "test:must:auditlogs": "./scripts/prepare-environment.sh && ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must and @AuditLog' features",
    "test:browser": "HEADLESS=false npm run test",
    "test:browser-noteardown": "NO_TEARDOWN=true npm run test:browser",
    "test:characterisation": "cd characterisation && jest",
    "test:characterisation:bichard": "cd characterisation && USE_BICHARD=true  jest --runInBand",
    "test:nextUI": "./scripts/prepare-environment.sh && nextUI=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@NextUI'",
    "test:nextUI:file": "./scripts/prepare-environment.sh && nextUI=true node --tls-min-v1.0 ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --tags '@NextUI'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-tests.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-tests/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-tests#readme",
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@cucumber/cucumber": "^8.7.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.32",
    "@types/jest": "^29.2.2",
    "aws-sdk": "^2.1248.0",
    "axios": "^1.1.3",
    "csv-parse": "^5.3.1",
    "date-fns": "^2.29.3",
    "expect": "^29.2.2",
    "fast-xml-parser": "^4.0.11",
    "jest": "^29.2.2",
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.3",
    "pg-promise": "^10.12.1",
    "promise-poller": "^1.9.1",
    "puppeteer": "^19.2.2",
    "puppeteer-mass-screenshots": "^1.0.15",
    "stompit": "^1.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "gherkin-lint": "^4.2.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier -w"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ]
  }
}
