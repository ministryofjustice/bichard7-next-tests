{
  "name": "bichard7-next-tests",
  "version": "1.0.0",
  "description": "Tests for Bichard",
  "main": "index.js",
  "scripts": {
    "lint:features": "gherkin-lint features",
    "lint:js": "eslint --fix .",
    "lint": "npm run lint:js && npm run lint:features",
    "prepare": "husky install",
    "test:browser-noteardown": "NO_TEARDOWN=true npm run test:browser",
    "test:browser": "HEADLESS=false npm run test",
    "test:characterisation:bichard": "cd characterisation && USE_BICHARD=true  jest --runInBand",
    "test:characterisation": "cd characterisation && jest --runInBand",
    "test:chunk:conductor": "bash ./scripts/run_test_chunk.sh 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and not @ExcludedOnConductor'",
    "test:chunk:nextUI": "nextUI=true bash ./scripts/run_test_chunk.sh 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC and @NextUI'",
    "test:chunk": "bash ./scripts/run_test_chunk.sh 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC'",
    "test:file": "./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter",
    "test:must": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must' features",
    "test:nextUI:file": "nextUI=true ./node_modules/.bin/cucumber-js --require steps --no-strict --exit --publish-quiet --tags '@NextUI'",
    "test:nextUI:must": "nextUI=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@Must and @NextUI' features",
    "test:nextUI:preprod": "SKIP_PNC_VALIDATION=true nextUI=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and @NextUI and not @Excluded' features",
    "test:nextUI": "nextUI=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@NextUI'",
    "test:parallel": "curl -X DELETE http://localhost:3000/mocks && ./node_modules/.bin/cucumber-js --require steps --retry 0 --no-strict --exit --publish-quiet --fail-fast --tags '@Parallel' --parallel $CONCURRENCY features",
    "test:preprod": "SKIP_PNC_VALIDATION=true ./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags '@PreProdTest and not @Excluded' features",
    "test": "./node_modules/.bin/cucumber-js --require steps --retry 5 --no-strict --exit --publish-quiet --format @cucumber/pretty-formatter --fail-fast --tags 'not @Excluded and not @ExcludedOnMaster and not @OnlyRunsOnPNC' features",
    "update-deps": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-tests.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-tests/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-tests#readme",
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@cucumber/cucumber": "^9.5.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.121",
    "@types/jest": "^29.5.5",
    "aws-sdk": "^2.1467.0",
    "axios": "^1.5.1",
    "csv-parse": "^5.5.0",
    "date-fns": "^2.30.0",
    "expect": "^29.7.0",
    "fast-xml-parser": "^4.3.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nunjucks": "^3.2.4",
    "pg-promise": "^11.5.4",
    "promise-poller": "^1.9.1",
    "puppeteer": "^21.3.6",
    "puppeteer-mass-screenshots": "^1.0.15",
    "stompit": "^1.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.0",
    "gherkin-lint": "^4.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier -w"
    ],
    "features/**/*.feature": [
      "gherkin-lint"
    ]
  }
}
